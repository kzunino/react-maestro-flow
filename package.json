{
	"name": "react-maestro",
	"version": "0.0.1",
	"description": "Stateful, branching workflow orchestration for React.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepublishOnly": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"playground": "npm run build && cd playground && npm run dev",
		"playground:install": "cd playground && npm install"
	},
	"keywords": [
		"react",
		"library"
	],
	"author": "",
	"license": "ISC",
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"tsup": "^8.5.1"
	},
	"type": "module"
}
